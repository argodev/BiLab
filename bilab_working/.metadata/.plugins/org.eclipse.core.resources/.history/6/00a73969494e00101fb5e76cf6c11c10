package bilab;

import org.eclipse.jface.viewers.ISelection;

import scigol.Value;

public class ValueSelection implements ISelection {
    public Value value;

    public ValueSelection(final Value v) {
        value = v;
    }

    @Override
    public boolean isEmpty() {
        return value == null;
    }
}
